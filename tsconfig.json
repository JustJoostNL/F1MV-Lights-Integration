{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "isolatedModules": true,
    "strict": true,
    "allowJs": true,
    "skipLibCheck": true,
    "jsx": "preserve",
    "forceConsistentCasingInFileNames": true,
    "useUnknownInCatchVariables": false,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    //"baseUrl": ".",
    "outDir": "./dist",
    "moduleResolution": "node",
    "resolveJsonModule": true
  }
}
